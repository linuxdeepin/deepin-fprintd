<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE policyconfig PUBLIC
 "-//freedesktop//DTD PolicyKit Policy Configuration 1.0//EN"
 "http://www.freedesktop.org/standards/PolicyKit/1.0/policyconfig.dtd">
<policyconfig>
  <vendor/>
  <vendor_url/>

  <action id="com.deepin.daemon.Fprintd.Enroll">
    <description>Enroll new fingerprints</description>
    <message>Privileges are required to enroll new fingerprints.</message>
    <defaults>
      <allow_any>no</allow_any>
      <allow_inactive>no</allow_inactive>
      <allow_active>auth_admin_keep</allow_active>
    </defaults>
  </action>
  <action id="com.deepin.daemon.Fprintd.Verify">
    <description>Verify a fingerprint</description>
    <message>Privileges are required to verify fingerprints.</message>
    <defaults>
      <allow_any>no</allow_any>
      <allow_inactive>no</allow_inactive>
      <allow_active>yes</allow_active>
    </defaults>
  </action>
  <action id="com.deepin.daemon.Fprintd.Delete">
    <description>Delete fingerprints</description>
    <message>Privileges are required to delete fingerprints.</message>
    <defaults>
      <allow_any>no</allow_any>
      <allow_inactive>no</allow_inactive>
      <allow_active>auth_admin_keep</allow_active>
    </defaults>
  </action>
</policyconfig>
